{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "react-understanding-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/reactUnderstanding/ReactUnderstandingWebPart.js",
          "manifest": "./src/webparts/reactUnderstanding/ReactUnderstandingWebPart.manifest.json"
        }
      ]
    },
    "functional-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/functionalForm/FunctionalFormWebPart.js",
          "manifest": "./src/webparts/functionalForm/FunctionalFormWebPart.manifest.json"
        }
      ]
    },
    "share-point-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sharePointForm/SharePointFormWebPart.js",
          "manifest": "./src/webparts/sharePointForm/SharePointFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ReactUnderstandingWebPartStrings": "lib/webparts/reactUnderstanding/loc/{locale}.js",
    "FunctionalFormWebPartStrings": "lib/webparts/functionalForm/loc/{locale}.js",
    "SharePointFormWebPartStrings": "lib/webparts/sharePointForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}